{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --cors --port 10626 --single",
    "backend": "json-server --watch db.json --routes routes.json --port 10627 --middlewares ./node_modules/json-server-reset",
    "quick": "concurrently \"npm run backend\" \"npm run dev\"",
    "test": "npx cypress open",
    "test:open": "npx cypress open --browser edge",
    "test:cypress-headed": "npx cypress --headed",
    "tsc:check": "npm run tsc:src && npm run tsc:cypress",
    "tsc:src": "tsc --noEmit --project tsconfig.json",
    "tsc:cypress": "tsc --noEmit --project cypress/tsconfig.json",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^1.5.0",
    "@cypress/code-coverage": "^3.9.2",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@testing-library/cypress": "^7.0.4",
    "concurrently": "^6.0.0",
    "cypress": "^6.8.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-testing-library": "^3.10.2",
    "istanbul-lib-coverage": "^3.0.0",
    "json-server": "^0.16.3",
    "json-server-reset": "^1.3.0",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rollup": "^2.42.3",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.35.0",
    "uuid": "^3.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "bulma": "^0.9.2",
    "sirv-cli": "^1.0.11",
    "svelte-routing": "^1.5.0"
  }
}
